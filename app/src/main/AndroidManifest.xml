<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Hanfu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hanfu">
        <activity android:name=".pages.QuestionTypeActivity"></activity>
        <activity android:name=".pages.SearchActivity" />
        <activity android:name=".pages.ShoppingActivity" />
        <activity android:name=".pages.PartyActivity" />
        <activity android:name=".pages.ArticleActivity" />
        <activity android:name=".pages.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>